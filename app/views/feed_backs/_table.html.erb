<% if objects.any? %>
  <table class="table table-striped table-bordered table-condensed">
    <thead>
      <tr>
        <th>Name</th>
        <th>email</th>
        <th>Mobile</th>
        <th>Message</th>
      </tr>
    </thead>
    <tbody>
      <% for feed_back in objects %>
        <tr class="<%= cycle :odd, :even %>">
          <td><%= feed_back.name %></td>
          <td><%= feed_back.email %></td>
          <td><%= feed_back.mobile %></td>
          <td><%= feed_back.message %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>